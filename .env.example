# Example environment variables for Arnav Travels Frontend
# Copy this file to .env and fill in your actual values

# Backend API URL (required)
REACT_APP_API_URL=http://localhost:3500

# Alternative name for API base (for compatibility)
REACT_APP_API_BASE=http://localhost:3500

# Environment setting
NODE_ENV=development

# Generate a new site ID at https://netlify.com for deployment
REACT_APP_NETLIFY_SITE_ID=your_netlify_site_id

# Google Analytics tracking ID (optional)
REACT_APP_GA_TRACKING_ID=your_ga_tracking_id
